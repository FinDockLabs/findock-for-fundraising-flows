        <locationX>974</locationX>
        <locationY>242</locationY>
        <defaultConnector>
            <targetReference>SelectActionScreen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Other</defaultConnectorLabel>
        <rules>
            <name>HasError</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>isError</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>DisplayErrorScheduleNotFoundScreen</targetReference>
            </connector>
            <label>Error</label>
        </rules>
        <rules>
            <name>HasScheduleAction</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>action</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>UpgradeDowngradeAction</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>action</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>UpdateGiftCommitmentScheduleDatesAction</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>action</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>UpdatePaymentMethodAction</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>GetGiftCommitmentSchedules</targetReference>
            </connector>
            <label>Has Schedule Action</label>
        </rules>
    </decisions>
    <decisions>
        <description>Determines whether to update the schedule dates, payment method, or perform upgrade or downgrade on the recurring gift commitment schedule that’s retrieved in GetGiftCommitmentSchedules.</description>
