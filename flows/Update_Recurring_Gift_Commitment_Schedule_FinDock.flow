    <formulas>
        <description>Validate EndDate formula for UpdateDates of gift commitment schedule.</description>
        <name>ValidateUpdateDatesEndDateFormula</name>
        <dataType>Boolean</dataType>
        <expression>IF(({!UpdateDatesStartDate}&gt;{!UpdateDatesEndDate}), false, true)</expression>
    </formulas>
    <formulas>
        <description>Validate EffectiveEndDate formula for UpdatePaymentMethod of gift commitment schedule.</description>
        <name>ValidateUpdatePaymentMethodEffectiveEndDateFormula</name>
        <dataType>Boolean</dataType>
        <expression>IF((({!UpdatePaymentMethodEffectiveEndDate}&lt;{!UpdatePaymentMethodEffectiveStartDate}) || ({!UpdatePaymentMethodEffectiveEndDate}&gt;{!UpdatePaymentMethodSelectSchdDataTable.firstSelectedRow.EndDate}) || ({!UpdatePaymentMethodEffectiveEndDate}&lt;{!UpdatePaymentMethodSelectSchdDataTable.firstSelectedRow.StartDate})) ,false, true)</expression>
    </formulas>
    <formulas>
        <description>Validate EffectiveStartDate formula for UpdatePaymentMethod of gift commitment schedule.</description>
        <name>ValidateUpdatePaymentMethodEffectiveStartDateFormula</name>
        <dataType>Boolean</dataType>
        <expression>IF((({!UpdatePaymentMethodEffectiveStartDate}&lt;{!UpdatePaymentMethodSelectSchdDataTable.firstSelectedRow.StartDate}) || ({!UpdatePaymentMethodEffectiveStartDate}&gt;{!UpdatePaymentMethodSelectSchdDataTable.firstSelectedRow.EndDate})) ,false, true)</expression>
    </formulas>
    <formulas>
        <description>Validate EffectiveEndDate formula for UpgradeDowngrade of gift commitment schedule.</description>
        <name>ValidateUpgradeDowngradeEffectiveEndDateFormula</name>
        <dataType>Boolean</dataType>
        <expression>IF((({!UpgradeDowngradeEffectiveEndDate}&lt;{!UpgradeDowngradeEffectiveStartDate}) || ({!UpgradeDowngradeEffectiveEndDate}&gt;{!UpgradeDowngradeSelectSchdDataTable.firstSelectedRow.EndDate}) || ({!UpgradeDowngradeEffectiveEndDate}&lt;{!UpgradeDowngradeSelectSchdDataTable.firstSelectedRow.StartDate})) ,false, true)</expression>
    </formulas>
    <formulas>
        <description>Validate EffectiveStartDate formula for UpgradeDowngrade of gift commitment schedule.</description>
        <name>ValidateUpgradeDowngradeEffectiveStartDateFormula</name>
        <dataType>Boolean</dataType>
        <expression>IF((({!UpgradeDowngradeEffectiveStartDate}&lt;{!UpgradeDowngradeSelectSchdDataTable.firstSelectedRow.StartDate}) || ({!UpgradeDowngradeEffectiveStartDate}&gt;{!UpgradeDowngradeSelectSchdDataTable.firstSelectedRow.EndDate})) ,false, true)</expression>
    </formulas>
    <formulas>
        <description>Validate TransactionAmount formula for UpgradeDowngrade of gift commitment schedule.</description>
        <name>ValidateUpgradeDowngradeTransactionAmountFormula</name>
        <dataType>Boolean</dataType>
